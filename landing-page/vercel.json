{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "redirects": [
    {
      "source": "/coverage",
      "destination": "/coverage/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/coverage/",
      "destination": "/coverage/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}